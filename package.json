{
  "name": "electron-resource-estimator",
  "version": "0.1.8",
  "main": "dist-electron/main/index.js",
  "description": "A simple hardware resource estimator tool for virtual environments ",
  "author": "dzwg",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "preBuild": "vue-tsc --noEmit && vite build",
    "build": "vue-tsc --noEmit && vite build && electron-builder",
    "release": "vue-tsc --noEmit && vite build && electron-builder --publish always"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@iconify/vue": "^4.0.0",
    "@types/jspdf": "^2.0.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "autoprefixer": "^10.4.13",
    "electron": "^21.1.0",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-electron-plugin": "^0.5.0",
    "vite-plugin-electron-renderer": "^0.11.3",
    "electron-builder": "^23.6.0",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.9"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@react-pdf/renderer": "^3.0.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vueuse/core": "^9.5.0",
    "daisyui": "^2.40.1",
    "downloadjs": "^1.4.7",
    
    "flowbite": "^1.5.4",
    "html2pdf.js": "^0.10.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.5.25",
    "jspdf-html2canvas": "^1.4.9",
    "mosha-vue-toastify": "^1.0.23",
    "pinia": "^2.0.23",
    "vue-json-pretty": "^2.1.0",
    "vue-router": "^4.1.6",
    "vue-uuid": "^3.0.0",
    "vue3-html2pdf": "^1.1.2"
  }
}
